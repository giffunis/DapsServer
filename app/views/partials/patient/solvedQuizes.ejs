<div class="dataTable_wrapper">
    <table class="table table-striped table-bordered table-hover" id="solvedQuizes">
        <thead>
            <tr>
                <th>Fecha</th>
                <th>Test</th>
                <th>Nº Preguntas</th>
                <th>Aciertos</th>
                <th>Fallos</th>
                <th>Duración(s)</th>
                <th>Link</th>
            </tr>
        </thead>
        <tbody>
            <tr class="gradeA">
                <td class="center"><%= quizes[i].date %></td>
                <td class="center"><%= quizes[i].quizName %></td>
                <td class="center"><%= quizes[i].nQuestions %></td>
                <td class="center"><%= quizes[i].correctAnswers %></td>
                <td class="center"><%= quizes[i].mistakes %></td>
                <td class="center"><%= quizes[i].time %></td>
                <td class="center"><a href="#">quizes[i]._id</a></td>
            </tr>

        </tbody>
    </table>
</div>
<!-- /.table-responsive -->
