<div class="dataTable_wrapper">
    <table class="table table-striped table-bordered table-hover" id="solvedQuizes">
        <thead>
            <tr>
                <th>Fecha</th>
                <th>Test</th>
                <th>Nº Preguntas</th>
                <th>Aciertos</th>
                <th>Fallos</th>
                <th>Duración(s)</th>
                <th>Link</th>
            </tr>
        </thead>
        <% if (solvedQuizes) { %>
          <tbody>
            <% for (var i = 0; i < solvedQuizes.length; i++) { %>
              <tr class="gradeA">
                  <td class="center"><%= solvedQuizes[i].date %></td>
                  <td class="center"><%= solvedQuizes[i].quizName %></td>
                  <td class="center"><%= solvedQuizes[i].nQuestions %></td>
                  <td class="center"><%= solvedQuizes[i].correctAnswers %></td>
                  <td class="center"><%= solvedQuizes[i].mistakes %></td>
                  <td class="center"><%= solvedQuizes[i].time %></td>
                  <td class="center"><a href="/patient/<%= patient._id %>/quiz/<%= solvedQuizes[i]._id %>">ver</a></td>
              </tr>
            <% } %>
          </tbody>
        <% } %>
    </table>
</div>
<!-- /.table-responsive -->
