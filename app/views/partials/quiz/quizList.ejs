<% if (quizes) { %>
  <ul>
    <% for(var i = 0; i < quizes.length; i++){ %>
      <li> Quiz <%= i + 1 %>: <a href="/quiz/<%= quizes[i]._id %>"><%- quizes[i].quizName %></a></li>
    <% } %>
  </ul>
<% } else { %>
  <div class="alert alert-info" role="alert"> No se encuentran resultados </div>
<% } %>
